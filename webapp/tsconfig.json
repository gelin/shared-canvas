{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["svelte", "vite/client"],
    "paths": {
      "$lib/*": ["./src/lib/*"]
    }
  },
  "include": [
    "./src/**/*",
    "./src/**/*.svelte"
  ]
}

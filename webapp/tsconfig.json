    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["svelte", "vite/client", "vite-plugin-pwa/info", "vite-plugin-pwa/svelte", "vite-plugin-pwa/client"],
  },
  "include": [
    "./src/**/*",
    "./src/**/*.svelte"
  ]
}
